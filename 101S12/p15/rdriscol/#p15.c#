/* Title: Solid Box of Asterisks  */
/*                                */
/* Approx Completion Time 30 min  */
/*                                */

#include <stdio.h>

int main(int argc, char * argv[] ){

  int i, j;

  printf( "Enter two numbers here" );

  for ( i = 1; i <= 4; i++)
  {
	  printf("*");

	  for ( j = 1; j <= 5; j++)
	  {
		  printf("\n");
	  }
  }
  return 0;

}
   
